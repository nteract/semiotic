{
  "name": "semiotic",
  "version": "2.0.0",
  "description": "The semiotic JavaScript data visualization framework",
  "main": "dist/semiotic.js",
  "module": "dist/semiotic.module.js",
  "types": "dist/semiotic.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --config jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:old": "node scripts/test.js --env=jsdom",
    "pretest:dist": "npm run dist && rm -rf screenshots && mkdir screenshots",
    "test:dist": "node scripts/test-dist.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prebuild": "babel src/components --extensions '.js,.jsx,.ts,.tsx' --out-dir lib --ignore test.js",
    "create-release-branch": "sh ./scripts/create-release-branch.sh",
    "publish-release": "sh ./scripts/publish-release.sh",
    "deploy-docs": "sh ./scripts/docs.sh",
    "ci-deploy-docs": "sh ./scripts/docs-ci.sh",
    "gh-pages": "REACT_APP_GH_PAGES_PATH='semiotic' npm run build",
    "typescript": "tsc --declaration --noEmit",
    "dist": "rollup --config rollup.config.js",
    "docs-bundle-stats": "GENERATE_SOURCEMAP='true' NODE_ENV='production' webpack --config ./config/webpack.config.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "prepublishOnly": "npm run typescript && npm run dist"
  },
  "repository": "nteract/semiotic",
  "author": {
    "name": "Elijah Meeks",
    "email": "elijahmeeks@gmail.com"
  },
  "contributors": [
    "Susie Lu <susie.lu.shan@gmail.com>"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@material-ui/core": "^4.8.1",
    "@playwright/test": "^1.17.1",
    "@types/d3-scale": "^2.1.1",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@vx/pattern": "0.0.143",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^27.4.5",
    "classnames": "2.2.5",
    "conventional-changelog-cli": "1.1.1",
    "conventional-recommended-bump": "0.3.0",
    "d3-dsv": "1.0.5",
    "d3-flextree-v4": "^1.0.1",
    "d3-random": "1.0.1",
    "d3-sankey": "0.7.1",
    "dagre": "^0.8.2",
    "dentist": "1.0.3",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^7.0.0",
    "eslint-config-netflix-dea": "^4.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-typescript": "^0.14.0",
    "history": "4.2.0",
    "in-publish": "2.0.0",
    "jest": "^27.4.5",
    "material-design-icons-svg": "1.1.2",
    "parcel": "^2.0.1",
    "playwright-chromium": "^1.17.1",
    "prettier": "^2.5.1",
    "react": "16.14.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.14.0",
    "react-element-to-jsx-string": "^12.0.0",
    "react-prism": "4.3.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16",
    "rollup": "^2.61.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-ts": "^2.0.4",
    "roughjs": "^3.1.0",
    "ts-jest": "^27.1.1",
    "tslib": "^1.9.3",
    "typescript": "3.9.7",
    "typescript-eslint-parser": "^22.0.0"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "d3-array": "^1.2.0",
    "d3-brush": "^2.1.0",
    "d3-chord": "^1.0.4",
    "d3-collection": "^1.0.1",
    "d3-contour": "^1.1.1",
    "d3-force": "^1.0.2",
    "d3-glyphedge": "^1.2.0",
    "d3-hexbin": "^0.2.2",
    "d3-hierarchy": "^1.1.3",
    "d3-interpolate": "^1.1.5",
    "d3-polygon": "^1.0.5",
    "d3-sankey-circular": "0.25.0",
    "d3-scale": "^1.0.3",
    "d3-selection": "2",
    "d3-shape": "^1.3.5",
    "d3-voronoi": "^1.0.2",
    "element-resize-event": "^3.0.3",
    "labella": "1.1.4",
    "memoize-one": "^5.1.1",
    "object-assign": "4.1.1",
    "polygon-offset": "0.3.1",
    "react-annotation": "3.0.0-beta.4",
    "regression": "^2.0.1",
    "semiotic-mark": "0.5.0",
    "svg-path-bounding-box": "1.0.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
