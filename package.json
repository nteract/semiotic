{
  "name": "semiotic",
  "version": "2.0.1",
  "description": "The semiotic JavaScript data visualization framework",
  "main": "dist/semiotic.js",
  "module": "dist/semiotic.module.js",
  "types": "dist/semiotic.d.ts",
  "sideEffects": false,
  "files": [
    "dist/semiotic.*"
  ],
  "scripts": {
    "start": "parcel serve --target docs",
    "build": "parcel build --target docs",
    "test": "jest --config jest.config.js",
    "pretest:dist": "npm run dist",
    "test:dist": "playwright test",
    "serve-examples": "npm run dist && parcel serve integration-tests/viz-examples/index.html",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "create-release-branch": "sh ./scripts/create-release-branch.sh",
    "publish-release": "sh ./scripts/publish-release.sh",
    "website:start": "npm run dist && parcel serve --target website",
    "website:build": "npm run dist && parcel build --target website",
    "typescript": "tsc --noEmit",
    "dist": "rollup --config rollup.config.js",
    "prepublishOnly": "npm run typescript && npm run dist"
  },
  "resolutions": {
    "@types/react": "^17.0.37"
  },
  "targets": {
    "website": {
      "context": "browser",
      "scopeHoist": false,
      "source": "docs/public/index.html",
      "distDir": "docs/build",
      "engines": {
        "browsers": "Firefox 60, Chrome 67, Safari 11.1"
      }
    },
    "docs": {
      "source": "public/index.html",
      "distDir": "build",
      "engines": {
        "browsers": "Firefox 60, Chrome 67, Safari 11.1"
      }
    }
  },
  "alias": {
    "semiotic": "."
  },
  "repository": "nteract/semiotic",
  "author": {
    "name": "Elijah Meeks",
    "email": "elijahmeeks@gmail.com"
  },
  "contributors": [
    "Susie Lu <susie.lu.shan@gmail.com>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@material-ui/core": "^4.8.1",
    "@mdx-js/react": "^1.6.22",
    "@parcel/transformer-inline-string": "^2.8.2",
    "@parcel/transformer-mdx": "^2.8.2",
    "@playwright/test": "^1.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3-scale": "^2.1.1",
    "@types/node": "^13.7.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.6",
    "@vx/pattern": "0.0.143",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^27.4.5",
    "classnames": "2.2.5",
    "conventional-changelog-cli": "1.1.1",
    "conventional-recommended-bump": "0.3.0",
    "d3-dsv": "1.0.5",
    "d3-flextree-v4": "^1.0.1",
    "d3-random": "1.0.1",
    "d3-sankey": "0.12.3",
    "dagre": "^0.8.2",
    "dentist": "1.0.3",
    "enzyme": "3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-netflix-dea": "^4.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-typescript": "^0.14.0",
    "history": "4.2.0",
    "in-publish": "2.0.0",
    "jest": "^27.4.5",
    "marked": "4.0.10",
    "material-design-icons-svg": "1.1.2",
    "parcel": "^2.8.2",
    "playwright-chromium": "^1.17.1",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-element-to-jsx-string": "^12.0.0",
    "react-prism": "4.3.2",
    "react-router-dom": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.79.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "roughjs": "^3.1.0",
    "ts-jest": "^27.1.1",
    "tslib": "^1.14.1",
    "typescript": "4.8.4",
    "typescript-eslint-parser": "^22.0.0"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
    "d3-array": "3.2.4",
    "d3-brush": "3.0.0",
    "d3-chord": "3.0.1",
    "d3-contour": "^1.1.1",
    "d3-delaunay": "^6.0.4",
    "d3-force": "3.0.0",
    "d3-glyphedge": "^1.2.0",
    "d3-hexbin": "^0.2.2",
    "d3-hierarchy": "3.1.2",
    "d3-interpolate": "3.0.1",
    "d3-path-arrows": "^0.4.0",
    "d3-polygon": "3.0.1",
    "d3-sankey-circular": "0.34.0",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-shape": "3.2.0",
    "dequal": "^2.0.3",
    "labella": "1.1.4",
    "memoize-one": "^5.1.1",
    "object-assign": "4.1.1",
    "polygon-offset": "0.3.1",
    "react-annotation": "3.0.0-beta.5",
    "regression": "^2.0.1",
    "svg-path-bounding-box": "1.0.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "Edge 17",
      "Firefox 60",
      "Chrome 67",
      "Safari 11.1",
      "IE 11"
    ]
  }
}
